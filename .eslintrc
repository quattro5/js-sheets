{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "linebreak-style": 0,
        // "indent": ["error", "tab"],
        "indent": "off",
        "comma-dangle": "off",
        "no-tabs": ["error", { "allowIndentationTabs": true }],
        "object-curly-spacing": "off",
        "require-jsdoc":"off",
        "arrow-parens":"off",
        "operator-linebreak":"off"
    },
    "extends": [
        "eslint:recommended", "google"
    ]
}
